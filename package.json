{
  "name": "presenterkit",
  "version": "0.0.1",
  "description": "PresenterKit",
  "author": "Ali Mammadli & Sevil Rasulova",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "pdf-img-convert": "^1.2.1",
    "ws": "^8.12.0"
  },
  "main": "main.js",
  "scripts": {
    "start": "cd client && npm start",
    "build:react": "cd client && npm start",
    "electron:start": "wait-on http://127.0.0.1:3000 && electron .",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:build": "npm run build:react && electron-builder",
    "rebuild": "npm rebuild canvas --update-binary"
  },
  "build": {
    "appId": "com.electron.presenterkit"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^30.0.1",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
  }
}
