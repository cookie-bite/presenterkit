{
  "name": "PresenterKit",
  "version": "0.0.1",
  "description": "PresenterKit",
  "author": "Ali Mammadli & Sevil Rasulova",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "pdf-img-convert": "^1.2.1",
    "ws": "^8.12.0"
  },
  "main": "main.js",
  "scripts": {
    "react": "cd client && npm start",
    "start": "wait-on http://127.0.0.1:3000 && electron .",
    "dev": "concurrently -k \"cross-env BROWSER=none npm run react\" \"cross-env NODE_ENV=development npm start\"",
    "dist": "cd client && npm run build",
    "build": "npm run dist && electron-builder",
    "rebuild": "npm rebuild canvas --update-binary"
  },
  "build": {
    "productName": "PresenterKit",
    "copyright": "Copyright Â© 2024 PresenterKit",
    "appId": "com.electron.presenterkit",
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "resources/icon.icns"
    },
    "win": {
      "icon": "resources/icon.ico"
    }
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^30.0.1",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
  }
}